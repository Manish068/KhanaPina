<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:left="2dp" android:top="5dp">
    <shape>
        <solid android:color="#03010C03"/>
        <corners android:radius="10dp"/>
    </shape>
</item>
    <item android:bottom="5dp" android:right="3dp">
        <shape android:shape="rectangle" >
            <corners
                android:radius="14dp"
                />
            <solid
                android:color="#F7F7F7"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <size
                android:width="100dp"
                android:height="100dp"
                />
            <stroke
                android:width="3dp"
                android:color="#EE8104"
                />
        </shape>
    </item>
</layer-list>